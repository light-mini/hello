<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mycompany.myapp2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:resizeableActivity="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.mycompany.myapp2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mycompany.myapp2.recycleactivity.RecycleViewActivity"
            android:label="@string/recycleview" />
        <activity
            android:name="com.mycompany.myapp2.recycleactivity.HorizontalViewActivity"
            android:label="@string/horizontalview" />
        <activity
            android:name="com.mycompany.myapp2.recycleactivity.LinearViewActivity"
            android:label="@string/linearview" />
        <activity
            android:name="com.mycompany.myapp2.recycleactivity.GridRecycleViewActivity"
            android:label="@string/gridrecycleview" />
        <activity
            android:name="com.mycompany.myapp2.recycleactivity.PuRecycleViewActivity"
            android:label="@string/purecycleview" />
        <activity
            android:name="com.mycompany.myapp2.WebViewActivity"
            android:label="@string/webview" />
        <activity
            android:name="com.mycompany.myapp2.ToastActivity"
            android:label="@string/toastview" />
        <activity
            android:name="com.mycompany.myapp2.Main2Activity"
            android:label="@string/main2view" />
        <activity
            android:name="com.mycompany.myapp2.UIActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.mycompany.myapp2.DialogActivity"
            android:label="@string/dialogview" />
        <activity
            android:name="com.mycompany.myapp2.ProgressActivity"
            android:label="@string/progressview" />
        <activity
            android:name="com.mycompany.myapp2.CustomDialogActivity"
            android:label="@string/customdialogview" />
        <activity
            android:name="com.mycompany.myapp2.PopupWindowActivity"
            android:label="@string/popupview" />
        <activity
            android:name="com.mycompany.myapp2.LiftCycleActivity"
            android:label="@string/liftcycleview" />
        <activity
            android:name="com.mycompany.myapp2.Jump.AActivity"
            android:label="A" />
        <activity
            android:name="com.mycompany.myapp2.Jump.BActivity"
            android:label="B" >
            <intent-filter>
                <action android:name="android.guangming.BActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>