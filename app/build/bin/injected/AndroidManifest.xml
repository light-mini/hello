<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.mycompany.myapp2"
	android:versionCode="1"
	android:versionName="1.0">

	<uses-sdk
		android:minSdkVersion="14"
		android:targetSdkVersion="21"/>

	<uses-permission
		android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

	<uses-permission
		android:name="android.permission.INTERNET"/>

	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme"
		android:resizeableActivity="true">

		<activity
			android:name=".MainActivity"
			android:label="@string/app_name">

			<intent-filter>

				<action
					android:name="android.intent.action.MAIN"/>

				<category
					android:name="android.intent.category.LAUNCHER"/>

			</intent-filter>

		</activity>

		<activity
			android:name=".recycleactivity.RecycleViewActivity"
			android:label="@string/recycleview"/>

		<activity
			android:name=".recycleactivity.HorizontalViewActivity"
			android:label="@string/horizontalview"/>

		<activity
			android:name=".recycleactivity.LinearViewActivity"
			android:label="@string/linearview"/>

		<activity
			android:name=".recycleactivity.GridRecycleViewActivity"
			android:label="@string/gridrecycleview"/>

		<activity
			android:name=".recycleactivity.PuRecycleViewActivity"
			android:label="@string/purecycleview"/>

		<activity
			android:name=".WebViewActivity"
			android:label="@string/webview"/>

		<activity
			android:name=".ToastActivity"
			android:label="@string/toastview"/>

		<activity
			android:name=".Main2Activity"
			android:label="@string/main2view"/>

		<activity
			android:name=".UIActivity"
			android:label="@string/app_name"/>

		<activity
			android:name=".DialogActivity"
			android:label="@string/dialogview"/>

		<activity
			android:name=".ProgressActivity"
			android:label="@string/progressview"/>

		<activity
			android:name=".CustomDialogActivity"
			android:label="@string/customdialogview"/>

		<activity
			android:name=".PopupWindowActivity"
			android:label="@string/popupview"/>

		<activity
			android:name=".LiftCycleActivity"
			android:label="@string/liftcycleview"/>

		<activity
			android:name=".Jump.AActivity"
			android:label="A"/>

		<activity
			android:name=".Jump.BActivity"
			android:label="B">

			<intent-filter>

				<action
					android:name="android.guangming.BActivity"/>

				<category
					android:name="android.intent.category.DEFAULT"/>

			</intent-filter>

		</activity>

	</application>

</manifest>

